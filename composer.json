{
    "name": "photobank/file-uploader-bundle",
    "description": "File uploading component implementing resumable js backend for symfony 4+",
    "type": "library",
    "autoload": {
        "psr-4": { "PhotoBank\\FileUploaderBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
      "php": "^7.1.3",
      "ext-ctype": "*",
      "ext-iconv": "*",
      "symfony/filesystem": "*",
      "symfony/finder": "*",
      "symfony/flex": "^1.1",
      "symfony/framework-bundle": "*",
      "symfony/orm-pack": "*",
      "symfony/security-bundle": "*",
      "symfony/serializer-pack": "*"
    },
    "authors": [
        {
            "name": "dimaefimov",
            "email": "dmitriy.efimov@yahoo.com"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
